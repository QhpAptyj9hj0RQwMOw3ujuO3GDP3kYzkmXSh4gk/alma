grammar {
    token TOP { <term> }

    token term:identifier {
        (<[a..z]>+)
        {
            my name = $0;
            return new Q.Term.Identifier { name };
        }
    }

    token term:my {
        "my" <.ws> (<[a..z]>+)
        {
            my name = $0;
            return new Q.Term.My {
                identifier: new Q.Identifier { name },
            };
        }
    }
}
